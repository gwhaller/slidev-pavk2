{
  "overrides": [
    {
      "files": ["slides.md", "pages/*.md"],
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "jsxSingleQuote": false,
        "htmlWhitespaceSensitivity": "ignore",
        "parser": "slidev",
        "plugins": ["prettier-plugin-slidev"]
      }
    }
  ]
}
